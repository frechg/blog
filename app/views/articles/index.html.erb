<header class="content-header">
  <h1>Articles</h1>
</header>
<section class="article-list">
  <% @articles.each do |article| %>
    <% unless article.archived? %>
      <article class="preview">
        <a href="<%= article_path(article) %>">
          <h3><%= article.title %></h3>
          <p></p>
        </a>
      </article>
    <% end %>
  <% end %>
</section>
